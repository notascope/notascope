{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "data/movies.csv"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "Major Genre",
      "type": "nominal"
    },
    "y": {
      "aggregate": "count",
      "stack": "normalize"
    },
    "color": {
      "field": "MPAA Rating",
      "type": "nominal"
    }
  }
}
