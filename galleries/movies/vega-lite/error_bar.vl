{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "data/movies.csv"
  },
  "encoding": {
    "y": {
      "field": "Major Genre"
    },
    "x": {
      "field": "Production Budget",
      "aggregate": "mean"
    }
  },
  "layer": [
    {
      "mark": "point"
    },
    {
      "mark": {
        "type": "errorbar",
        "extent": "stderr"
      }
    }
  ]
}
