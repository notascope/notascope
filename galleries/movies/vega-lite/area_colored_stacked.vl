{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "data/movies.csv"
  },
  "mark": "area",
  "encoding": {
    "x": {
      "field": "Release Date",
      "type": "temporal",
      "timeUnit": "year"
    },
    "y": {
      "field": "Worldwide Gross",
      "aggregate": "sum",
      "type": "quantitative",
      "stack": "normalize"
    },
    "color": {
      "field": "MPAA Rating",
      "type": "nominal"
    }
  }
}
